{
  "name": "trackbot-match",
  "private": true,
  "license": "AGPLv3-only",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "tilasify": "tsc --module nodenext --outDir build script/tilasify.mts && echo running... && node build/tilasify.mjs",
    "fetchMajors": "tsc --module nodenext --outDir build script/fetchMajors.mts && echo running... && node build/fetchMajors.mjs",
    "biggestPbs": "tsc --module nodenext --outDir build script/biggestPbs.mts && echo running... && node build/biggestPbs.mjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run jest && npm run build"
  },
  "prettier": {
    "printWidth": 160,
    "singleQuote": true
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@glaivepro/wa-calculator": "^0.3.0",
    "@mantine/core": "^6.0.10",
    "@mantine/form": "^5.10.5",
    "@mantine/hooks": "^6.0.10",
    "@mantine/modals": "^6.0.10",
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.8.2",
    "tabler-icons-react": "^1.56.0",
    "wikibase-edit": "^6.0.3",
    "wikibase-sdk": "^9.2.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/jsdom": "^21.1.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-mantine": "2.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "^21.1.2",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}
